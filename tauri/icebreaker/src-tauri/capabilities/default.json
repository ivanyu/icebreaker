{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    "http:allow-fetch",
    {
      "identifier": "http:default",
      "allow": [{ "url": "http://*:*/*" }, { "url": "https://*:*/*" }]
    }
  ]
}
